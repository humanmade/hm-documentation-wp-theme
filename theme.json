	"styles": {
		"blocks": {
			"core/code": {
				"border": {
					"radius": "6px"
				},
				"color": {
					"background": "#f6f8fa"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--x-large)",
						"top": "var(--wp--preset--spacing--x-large)"
					},
					"padding": {
						"bottom": "var(--wp--preset--spacing--small)",
						"left": "var(--wp--preset--spacing--regular)",
						"right": "var(--wp--preset--spacing--regular)",
						"top": "var(--wp--preset--spacing--small)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-mono)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "1.5"
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--x-large)"
					}
				}
			},
			"core/list": [],
			"core/paragraph": [],
			"core/preformatted": {
				"border": {
					"radius": "6px"
				},
				"color": {
					"background": "#f6f8fa"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--small)",
						"left": "var(--wp--preset--spacing--regular)",
						"right": "var(--wp--preset--spacing--regular)",
						"top": "var(--wp--preset--spacing--small)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-mono)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "1.5"
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--mega)",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "1.2"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--regular)"
				}
			},
			"heading": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.4"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "#084d99"
					}
				},
				":visited": {
					"color": {
						"text": "#5a3696"
					}
				},
				"color": {
					"text": "#0a66c2"
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--regular)",
			"padding": {
				"left": "var(--wp--preset--spacing--x-large)",
				"right": "var(--wp--preset--spacing--x-large)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"fontSize": "var(--wp--preset--font-size--regular)",
			"lineHeight": "1.4"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}