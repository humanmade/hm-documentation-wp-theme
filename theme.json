{
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "65ch",
			"wideSize": "110ch"
		},
		"spacing": {
			"units": [ "%", "px", "em", "rem", "vh", "vw" ],
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{ "name": "X Small", "slug": "x-small", "size": "0.5rem" },
				{ "name": "Small", "slug": "small", "size": "0.75rem" },
				{ "name": "Regular", "slug": "regular", "size": "1rem" },
				{ "name": "Large", "slug": "large", "size": "1.5rem" },
				{ "name": "Extra Large", "slug": "extra-large", "size": "2rem" },
				{ "name": "Huge", "slug": "huge", "size": "4rem" }
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, 'Liberation Mono', 'Courier New', monospace",
					"name": "System Monospace",
					"slug": "system-mono"
				}
			],
			"fontSizes": [
				{ "name": "Tiny", "size": "0.5rem", "slug": "tiny" },
				{ "name": "X Small", "size": "0.75rem", "slug": "x-small" },
				{ "name": "Small", "size": "0.875rem", "slug": "small" },
				{ "name": "Regular", "size": "1rem", "slug": "regular" },
				{ "name": "Large", "size": "1.25rem", "slug": "large" },
				{ "name": "Extra Large", "size": "1.5rem", "slug": "extra-large" },
				{ "name": "Huge", "size": "2rem", "slug": "huge" }
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"css": ":where(.wp-site-blocks) .wp-block-list > li + li{margin-top:var(--wp--preset--spacing--tiny);} :where(.wp-site-blocks) .wp-block-list .wp-block-list{margin-inline-start:var(--wp--preset--spacing--large);} :where(.wp-site-blocks) .wp-block-list .wp-block-list > li + li{margin-top:var(--wp--preset--spacing--x-small);}",
		"typography": {
			"fontFamily": "var:preset|font-family|system-font",
			"fontSize": "var:preset|font-size|regular",
			"lineHeight": "1.4"
		},
		"color": {
			"text": "#111111"
		},
		"spacing": {
			"blockGap": "var:preset|spacing|regular",
			"padding": { "left": "var:preset|spacing|extra-large", "right": "var:preset|spacing|extra-large" }
		},
		"elements": {
			"link": {
				"typography": { "textDecoration": "underline" },
				"color": { "text": "#0a66c2" },
				":hover": { "color": { "text": "#084d99" } },
				":visited": { "color": { "text": "#5a3696" } }
			},
			"heading": {
				"typography": { "lineHeight": "1.4" }
			},
			"h1": {
				"typography": { "fontSize": "var:preset|font-size|huge", "lineHeight": "1.25" },
				"spacing": { "margin": { "top": "var:preset|spacing|extra-large", "bottom": "var:preset|spacing|small" } }
			},
			"h2": {
				"typography": { "fontSize": "var:preset|font-size|extra-large", "lineHeight": "1.28" },
				"spacing": { "margin": { "top": "var:preset|spacing|large", "bottom": "var:preset|spacing|small" } }
			},
			"h3": {
				"typography": { "fontSize": "var:preset|font-size|large", "lineHeight": "1.3" },
				"spacing": { "margin": { "top": "var:preset|spacing|regular", "bottom": "var:preset|spacing|x-small" } }
			},
			"h4": {
				"typography": { "fontSize": "var:preset|font-size|regular", "fontWeight": "700", "lineHeight": "1.4" }
			},
			"h5": {
				"typography": { "fontSize": "var:preset|font-size|regular", "fontWeight": "700", "lineHeight": "1.4" }
			},
			"h6": {
				"typography": { "fontSize": "var:preset|font-size|x-small", "fontWeight": "700", "lineHeight": "1.4" }
			}
		},
		"blocks": {
			"core/code": {
				"typography": { "fontFamily": "var:preset|font-family|system-mono", "fontSize": "0.9375rem", "lineHeight": "1.5" },
				"color": { "background": "#f6f8fa" },
				"spacing": { "padding": { "top": "var:preset|spacing|small", "right": "var:preset|spacing|regular", "bottom": "var:preset|spacing|small", "left": "var:preset|spacing|regular" } },
				"border": { "radius": "6px" }
			},
			"core/preformatted": {
				"typography": { "fontFamily": "var:preset|font-family|system-mono", "fontSize": "0.9375rem", "lineHeight": "1.5" },
				"color": { "background": "#f6f8fa" },
				"spacing": { "padding": { "top": "var:preset|spacing|small", "right": "var:preset|spacing|regular", "bottom": "var:preset|spacing|small", "left": "var:preset|spacing|regular" } },
				"border": { "radius": "6px" }
			},
			"core/paragraph": {
				"spacing": { "margin": { "top": "0", "bottom": "var:preset|spacing|regular" } }
			},
			"core/list": {
				"spacing": { "margin": { "top": "var:preset|spacing|small", "bottom": "var:preset|spacing|regular" }, "padding": { "left": "0" } }
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "page-sidebar",
			"title": "Page with Sidebar",
			"postTypes": [ "page" ]
		}
	],
	"version": 3
}
